  </div>

  <div class="field">
    <%= f.label :recordable_type %>
    <%= f.text_field :recordable_type %>
  </div>

  <div class="field">
    <%= f.label :action %>
    <%= f.text_field :action %>
  </div>

  <div class="field">
    <%= f.label :logable_id %>
